function _init()
 create_player()
 init_msg()
end
 
function _update()
 if not messages[1] then
		player_mouvement()
	end
	update_camera()
	update_msg()
end

function _draw()
	cls()
	draw_map()
	draw_player()
	interface()
	draw_msg()
end


