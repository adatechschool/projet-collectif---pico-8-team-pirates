--player
function create_player()
	p={
				x=6,
				y=4,
				ox=0,
				oy=0,
				start_ox=0,
				start_oy=0,
				anim_timer=0,
				sprite=1, 
				keys=0
			}
end

function player_mouvement()
	newx=p.x
	newy=p.y 
	if btnp(➡️) then
		newx+=1
		newox=-8
		newoy=0
	elseif btnp(⬅️) then
	 newx-=1
	 newox=8
	 newoy=0
	elseif btnp(⬆️) then
	 newy-=1
	 newox=0
	 newoy=8
	elseif btnp(⬇️) then
	 newy+=1
	 newox=0
	 newoy=-8
	end
	
	interact(newx,newy)
	
	if not check_flag(0,newx,newy)
	and (p.x!=newx or p.y!=newy) then
		p.x=newx
		p.y=newy
		p.start_ox=newox
		p.start_oy=newoy
		p.anim_timer=1
	end
	
	p.anim_timer=max(p.anim_timer-0.125,0)
 p.ox=p.start_ox*p.anim_timer
 p.oy=p.start_oy*p.anim_timer
end



function interact(x,y)
	if check_flag(1,x,y) then
		pick_up_key(x,y)
	end
end

function interact(x,y)
	if check_flag(1,x,y) then
		pick_up_key(x,y)
	elseif check_flag(2,x,y)
	and p.keys>0 then
		open_door(x,y)
	end
	if x==7 and y==3 then
		create_msg("panneau",
	 "trouve les 3 cles\net tu pourra ouvrir le coffre") 
	end
			if p.keys==3 then
				if x==3 and y==2 then
					sprite=mget(x,y)
					if sprite==23 then
			 		next_title(x,y)
			 		p.keys = p.keys -3
		 		end
				end	
			end
end

function draw_player()
 	spr(p.sprite,p.x*8,p.y*8)
 	spr(25,p.x*8+p.ox,p.y*8+p.oy)
end
