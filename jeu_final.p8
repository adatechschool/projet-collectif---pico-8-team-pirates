function create_msg(name,...)
	msg_title=name
	messages={...}
end

function update_msg()
	if (btn(❎)) then
		deli(messages,1)
	end
end

function draw_msg()
	if messages[1] then
	 local y=100 
  rectfill(7,y,11+#msg_title*4,
	 y+7,4)
		print(msg_title,10,y+2,9)
  rectfill(3,y+8,124,y+24,5)
 	rect(3,y+8,124,y+24,8)
		print(messages[1],7,y+11,6)
	end 
end
